<div class="container" id="new-user-form">
    <table class="table is-responsive">
        <thead>
            <tr>
                <th>Whiskey</th>
                <th>Type</th>
                <th>Price</th>
                <th>Review</th>
                <th>Rating</th>
            </tr>
        </thead>
        <tbody>
            {{#each whiskeys}}
            <tr>
                <td>{{name}}</td>
                <td>{{type}}</td>
                <td>{{price}}</td>
                <td>{{body}}</td>
                <td>{{rating}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<script src="js/user.js" type="text/javascript"></script>